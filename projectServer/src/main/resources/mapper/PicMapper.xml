<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.server.mapper.ArticleMapper">
    <delete id="deleteByPoiId" parameterType="int">
        DELETE
        FROM pic_table
        WHERE
            poi_id = #{poiId}
    </delete>
</mapper>